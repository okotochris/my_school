                </fieldset>
                <fieldset class="studentInfo">
                    <legend>STUDENT INFO:</legend>
                    <p><input type="text" placeholder="Student Name" name="userName" required="required"/></p>
                    <p><input type="password" Placeholder="Student ID" name="studentId" required="required"/></p>
                    <p>
                        <p id="respond" style="color:red"></p>
                        <label>CLASS</label>:
                        <select name="class" id="Sclass">
                            <option></option>
                            <option>SS1</option>
                            <option>SS2</option>
                            <option>SS3</option>
                        </select id="Sterm">
                        <label>SCHOOL TERM</label>:
                        <select name="term">
                            <option></option>
                            <option>FIRST TERM</option>
                            <option>SECNOND TERM</option>
                            <option>THRID TERM</option>
                        </select>
                    </p>
                </fieldset>
                <fieldset class="schoolInfo">
                    <legend>School Info:</legend>
                    <p><input type="text" placeholder="School name" name="schoolName"/></p>
                    <input type="text" placeholder="Class teacher report" name="Treport">
                    <p><input type="text" Placeholder="Address" name="schoolAdd"/></p>
                    <select name="section" required>
                        <option value="">Academic Section</option>
                        <option value="2023 - 2024">2023 - 2024</option>
                        <option value="2024 - 2025">2024 - 2025</option>
                        <option value="2025 - 2026">2025 - 2026</option>
                        <option value="2026 - 2027">2026 - 2027</option>
                    </select>
                <button>Upload Result</button>
                </fieldset>
                <p id="upload" style="color:red;"></p>
            </form>
        </div>
        <script src="../main.js"></script>
