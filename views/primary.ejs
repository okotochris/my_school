                    <p><input type="text" value="Mathematics" name="mth"/><input type="text" placeholder="1st Text" name="mth1st"/><input type="text" placeholder="2st Text" name="mth2nd"/><input type="text" placeholder="Exam" name="mthexam"/><input type="text" placeholder="TOTAL SCORE" name="mthscore"/><input type="text" placeholder="GRADE" name="mthG"/></p>
                    <p><input type="text" value="Religion & National Value" name="rnv"/><input type="text" placeholder="1st Text" name="rnv1st"/><input type="text" placeholder="2st Text" name="rnv2nd"/><input type="text" placeholder="Exam" name="rnvexam"/><input type="text" placeholder="TOTAL SCORE" name="rnvscore"/><input type="text" placeholder="GRADE" name="rnvG"/></p>
                    <p><input type="text" value="Basic Science & Technology" name="bst"/><input type="text" placeholder="1st Text" name="bst1st"/><input type="text" placeholder="2st Text" name="bst2nd"/><input type="text" placeholder="Exam" name="bstexam"/><input type="text" placeholder="TOTAL SCORE" name="bstscore"/><input type="text" placeholder="GRADE" name="bstG"/></p>
                    <p><input type="text" value="Pre Vocatinal Studies" name="pvs"/><input type="text" placeholder="1st Text" name="pvs1st"/><input type="text" placeholder="2st Text" name="pvs2nd"/><input type="text" placeholder="Exam" name="pvsexam"/><input type="text" placeholder="TOTAL SCORE" name="pvsscore"/><input type="text" placeholder="GRADE" name="pvsG"/></p>
                    <p><input type="text" value="Quantitative Reasoning" name="qur"/><input type="text" placeholder="1st Text" name="qur1st"/><input type="text" placeholder="2st Text" name="qur2nd"/><input type="text" placeholder="Exam" name="qurexam"/><input type="text" placeholder="TOTAL SCORE" name="qurscore"/><input type="text" placeholder="GRADE" name="qurG"/></p>
                    <p><input type="text" value="Verbal Reasoning" name="ver"/><input type="text" placeholder="1st Text" name="ver1st"/><input type="text" placeholder="2st Text" name="ver2nd"/><input type="text" placeholder="Exam" name="verexam"/><input type="text" placeholder="TOTAL SCORE" name="verscore"/><input type="text" placeholder="GRADE" name="verG"/></p>
